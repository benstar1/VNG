	<parameter name="paramexo" class="java.lang.String"/>
	<parameter name="paramcentre" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT DISTINCT
     SUM(TOTAL_VCAF) AS totalvalcaf,
     SUM(TOTAL_DD) AS totaldd,
     SUM(TOTAL_TVA) AS totaltva,
     SUM(TOTAL_PC) AS totalpc,
     SUM(TOTAL_PCS) AS totalpcs,
     SUM(TOTAL_RS) AS totalrs,
     SUM(TOTAL_AIB) AS totalaib,
     T_ENT_DECLARATION."ENT_DEC_CONT_IMMATR" AS IFU,
     T_REP_UNIQUE."CONT_NOM" AS Nom_et_prenoms,
     T_REP_UNIQUE."CONT_RAIS" AS Raison_sociale
FROM
     "T_ENT_DECLARATION" T_ENT_DECLARATION INNER JOIN "T_DECLARATION_FISCALE" T_DECLARATION_FISCALE ON T_ENT_DECLARATION."ENT_DEC_NUM" = T_DECLARATION_FISCALE."DECLAR_ENT_DEC_NUM"
     INNER JOIN "T_REP_UNIQUE" T_REP_UNIQUE ON T_ENT_DECLARATION."ENT_DEC_CONT_IMMATR" = T_REP_UNIQUE."CONT_IMMATR"
WHERE
     EXO_ANNE =$P{paramexo}  AND CONT_CENTR_IMP_CODE = $P{paramcentre}
GROUP BY
     ENT_DEC_CONT_IMMATR,
     CONT_NOM,
     CONT_RAIS]]>
	</queryString>
