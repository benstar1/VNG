	<queryString>
		<![CDATA[SELECT
     TO_CHAR(T_DECLARATION_DOU."IDE_REG_DAT",'YYYYmmDD') || T_DECLARATION_DOU."IDE_REG_SER" || T_DECLARATION_DOU."IDE_REG_NBR" AS ID,	
     T_TAXE_DECL_DOU."KEY_ITM_NBR" AS NUMERO_ARTICLE,
     T_TAXE_DECL_DOU."KEY_TAX_RNK" AS NUMERO_TAXE,
     T_DECLARATION_DOU."IDE_RCP_SER" || T_DECLARATION_DOU."IDE_RCP_NBR" AS NUMERO_QUIT,
     T_DECLARATION_DOU."IDE_RCP_DAT" AS DATE_QUIT,
     T_DECLARATION_DOU."IDE_REG_SER" || T_DECLARATION_DOU."IDE_REG_NBR" AS NUMERO_DECL,
     T_DECLARATION_DOU."IDE_REG_DAT" AS DATE_ENREG,
     T_DECLARATION_DOU."CMP_EXP_COD" || T_DECLARATION_DOU."CMP_CON_COD" AS ENTREPRISE,
     T_DECLARATION_DOU."CMP_EXP_NAM" || T_DECLARATION_DOU."CMP_CON_NAM" AS NOM_ENTREPRISE,
     T_TAXE_DECL_DOU."TAX_LIN_COD" AS TAXE_CODE,
     T_TAXE_DECL_DOU."TAX_LIN_BSE" AS BASE_TAXABLE,
     T_TAXE_DECL_DOU."TAX_LIN_RAT" AS TAUX,
     T_TAXE_DECL_DOU."TAX_LIN_AMT" AS MONTANT_TAXE,
     T_DECLARATION_DOU."IDE_CUO_COD" AS CODE_BUREAU,
     T_DECLARATION_DOU."IDE_CUO_NAM" AS NOM_BUREAU,
     T_ARTICLE."PCK_NBR" AS NBRE_COLIS
FROM
     "PROP_REP_UNIQUE"."T_DECLARATION_DOU" T_DECLARATION_DOU,
     "PROP_REP_UNIQUE"."T_ARTICLE" T_ARTICLE,
     "PROP_REP_UNIQUE"."T_TAXE_DECL_DOU" T_TAXE_DECL_DOU
WHERE T_DECLARATION_DOU."INSTANCEID" = T_ARTICLE."INSTANCEID" 
AND T_ARTICLE."INSTANCEID" = T_TAXE_DECL_DOU."INSTANCEID"
AND T_TAXE_DECL_DOU."KEY_ITM_NBR" = T_ARTICLE."KEY_ITM_NBR"
ORDER BY TO_CHAR(T_DECLARATION_DOU."IDE_REG_DAT",'YYYYmmDD') || T_DECLARATION_DOU."IDE_REG_SER" || T_DECLARATION_DOU."IDE_REG_NBR", NUMERO_ARTICLE, T_TAXE_DECL_DOU."KEY_TAX_RNK"]]>
	</queryString>
