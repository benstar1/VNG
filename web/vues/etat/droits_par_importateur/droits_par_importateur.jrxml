	<queryString>
		<![CDATA[SELECT distinct
     T_DECLARATION_DOU."IDE_REG_SER" || T_DECLARATION_DOU."IDE_REG_NBR" AS NUM_DECL,
     T_DECLARATION_DOU."IDE_REG_DAT" AS DATE_ENR,    
     T_DECLARATION_DOU."IDE_RCP_SER" || T_DECLARATION_DOU."IDE_RCP_NBR" AS NUM_QUIT,
     T_DECLARATION_DOU."IDE_RCP_DAT" AS DATE_QUIT,  
     T_DECLARATION_DOU."DEC_COD" AS CODE_DECLARANT,
     replace(dbms_lob.substr(T_DECLARATION_DOU."DEC_NAM"),chr(10),'') AS NOM_DECLARANT,
     T_DECLARATION_DOU."IDE_CUO_COD" AS BUREAU,
     T_DECLARATION_DOU."DEC_REF_YER" AS ANNEE,
     T_ARTICLE."KEY_ITM_NBR" AS NUM_ART,
     T_ARTICLE."VIT_STV" AS VALEUR_ART,   
     T_ARTICLE."TAX_AMT" AS DROITS_ART,
     T_REP_UNIQUE."CONT_IMMATR" AS IFU,
     T_REP_UNIQUE."CONT_NOM" AS NOM_CONTRIB,
     T_REP_UNIQUE."CONT_PREN" AS PREN_CONTRIB,
     T_REP_UNIQUE."CONT_RAIS" AS RAIS_SOC,
     T_REP_UNIQUE."CONT_NOM_COURT" AS NOM_COURT,
     (SELECT TAXES."TAX_LIN_AMT"
     FROM "PROP_REP_UNIQUE"."T_TAXE_DECL_DOU" TAXES 
     where TAXES."INSTANCEID"=T_DECLARATION_DOU."INSTANCEID" 
     and T_ARTICLE."KEY_ITM_NBR" = TAXES."KEY_ITM_NBR" 
     and TAXES."TAX_LIN_COD" ='PC') as PC,
     (SELECT TAXES."TAX_LIN_AMT"
     FROM "PROP_REP_UNIQUE"."T_TAXE_DECL_DOU" TAXES 
     where TAXES."INSTANCEID"=T_DECLARATION_DOU."INSTANCEID" 
     and T_ARTICLE."KEY_ITM_NBR" = TAXES."KEY_ITM_NBR" 
     and TAXES."TAX_LIN_COD" ='PCS') as PCS,
     (SELECT TAXES."TAX_LIN_AMT"
     FROM "PROP_REP_UNIQUE"."T_TAXE_DECL_DOU" TAXES 
     where TAXES."INSTANCEID"=T_DECLARATION_DOU."INSTANCEID" 
     and T_ARTICLE."KEY_ITM_NBR" = TAXES."KEY_ITM_NBR" 
     and TAXES."TAX_LIN_COD" ='AIB') as AIB,
     (SELECT TAXES."TAX_LIN_AMT"
     FROM "PROP_REP_UNIQUE"."T_TAXE_DECL_DOU" TAXES 
     where TAXES."INSTANCEID"=T_DECLARATION_DOU."INSTANCEID" 
     and T_ARTICLE."KEY_ITM_NBR" = TAXES."KEY_ITM_NBR" 
     and TAXES."TAX_LIN_COD" ='AFS') as AFS,
     (SELECT TAXES."TAX_LIN_AMT"
     FROM "PROP_REP_UNIQUE"."T_TAXE_DECL_DOU" TAXES 
     where TAXES."INSTANCEID"=T_DECLARATION_DOU."INSTANCEID" 
     and T_ARTICLE."KEY_ITM_NBR" = TAXES."KEY_ITM_NBR" 
     and TAXES."TAX_LIN_COD" ='DD') as DD,
     (SELECT TAXES."TAX_LIN_AMT"
     FROM "PROP_REP_UNIQUE"."T_TAXE_DECL_DOU" TAXES 
     where TAXES."INSTANCEID"=T_DECLARATION_DOU."INSTANCEID" 
     and T_ARTICLE."KEY_ITM_NBR" = TAXES."KEY_ITM_NBR" 
     and TAXES."TAX_LIN_COD" ='TVA') as TVA
FROM "PROP_REP_UNIQUE"."T_DECLARATION_DOU" T_DECLARATION_DOU, "PROP_REP_UNIQUE"."T_ARTICLE" T_ARTICLE, 
	 "PROP_REP_UNIQUE"."T_TAXE_DECL_DOU" T_TAXE_DECL_DOU, "PROP_REP_UNIQUE"."T_REP_UNIQUE" T_REP_UNIQUE,"PROP_REP_UNIQUE"."T_CENTRE_IMPOT" T_CENTRE_IMPOT
WHERE T_DECLARATION_DOU."INSTANCEID" = T_ARTICLE."INSTANCEID" 
AND T_ARTICLE."INSTANCEID" = T_TAXE_DECL_DOU."INSTANCEID"
AND (T_DECLARATION_DOU."CMP_CON_COD" = T_REP_UNIQUE."CONT_IMMATR" OR T_DECLARATION_DOU."CMP_EXP_COD" = T_REP_UNIQUE."CONT_IMMATR")
AND T_ARTICLE."KEY_ITM_NBR" = T_TAXE_DECL_DOU."KEY_ITM_NBR" 
AND T_TAXE_DECL_DOU."TAX_LIN_COD" IN ('PC','PCS','AIB','AFS','DD','TVA') 
AND ( ($P{type} = 1 and T_DECLARATION_DOU."CMP_CON_COD" LIKE $P{ifu} ) 
       OR 
      ($P{type} = 2 and T_DECLARATION_DOU."CMP_EXP_COD" LIKE $P{ifu})
       OR 
      (((T_DECLARATION_DOU."CMP_EXP_COD" LIKE $P{ifu} OR T_DECLARATION_DOU."CMP_CON_COD" LIKE $P{ifu} ) AND $P{type} = 0)))
AND T_DECLARATION_DOU."IDE_RCP_DAT" BETWEEN $P{dateDeb} AND $P{dateFin}]]>
	</queryString>
