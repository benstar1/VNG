     "PROP_REP_UNIQUE"."T_ENT_DECLARATION" T_ENT_DECLARATION INNER JOIN "PROP_REP_UNIQUE"."T_DECLARATION_FISCALE" T_DECLARATION_FISCALE ON T_ENT_DECLARATION."ENT_DEC_NUM" = T_DECLARATION_FISCALE."DECLAR_ENT_DEC_NUM"
